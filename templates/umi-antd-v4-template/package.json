{
  "name": "umi-antd-v4-template",
  "version": "1.0.0",
  "description": "ant design v4版本模板项目",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "start": "umi dev",
    "build": "umi build",
    "test": "umi test",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "yarn:install": "yarn install"
  },
  "dependencies": {
    "@ant-design/icons": "4.0.0-rc.0",
    "@ant-design/pro-layout": "4.10.11-rc.6",
    "@uform/antd": "2.0.0",
    "antd": "4.0.0-rc.2",
    "feign-boot-browser-stater": "^1.0.0",
    "fengwuxp-babel-dependency": "^1.0.0",
    "fengwuxp-browser-router": "^1.0.0",
    "fengwuxp-browser-storage": "^1.0.0",
    "fengwuxp-common-utils": "^1.0.0",
    "fengwuxp-declarative-router-adapter": "^1.0.0",
    "fengwuxp-jest-dependency": "^1.0.0",
    "fengwuxp-react-dependency": "^1.0.0",
    "fengwuxp-routing-antd": "^1.0.0",
    "fengwuxp-routing-core": "^1.0.0",
    "fengwuxp-styles-dependency": "^1.0.0",
    "fengwuxp-typescipt-dependency": "^1.0.0",
    "fengwuxp-typescript-feign": "^1.0.0",
    "fengwuxp-webpack-4.x-dependency": "^1.0.0",
    "oak-common": "^1.0.0"
  },
  "devDependencies": {
    "@types/react-test-renderer": "^16.0.3",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "react-test-renderer": "^16.7.0",
    "slash2": "^2.0.0",
    "umi": "^2.9.0",
    "umi-plugin-antd-icon-config": "^1.0.2",
    "umi-plugin-antd-theme": "^1.0.15",
    "umi-plugin-locale": "^2.11.4",
    "umi-plugin-pro-block": "^1.3.6",
    "umi-plugin-react": "^1.15.2",
    "umi-types": "^0.3.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}

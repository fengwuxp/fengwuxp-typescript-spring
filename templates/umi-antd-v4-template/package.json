{
  "name": "umi-antd-v4-template",
  "version": "1.0.0",
  "description": "ant design v4版本模板项目",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "start": "cross-env UMI_ENV=local umi dev",
    "build": "umi build",
    "test": "umi test",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "yarn:install": "yarn install"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-layout": "^5.0.8",
    "@ant-design/pro-table": "2.2.1",
    "@formily/antd": "^1.1.2",
    "@formily/antd-components": "^1.1.2",
    "antd": "^4.0.0",
    "classnames": "^2.2.6",
    "feign-boot-browser-stater": "^1.0.0",
    "fengwuxp-babel-dependency": "^1.0.0",
    "fengwuxp-browser-router": "^1.0.0",
    "fengwuxp-browser-storage": "^1.0.0",
    "fengwuxp-common-utils": "^1.0.0",
    "fengwuxp-declarative-router-adapter": "^1.0.0",
    "fengwuxp-declarative-storage-adapter": "^1.0.0",
    "fengwuxp-event-state": "^1.0.0",
    "fengwuxp-jest-dependency": "^1.0.0",
    "fengwuxp-react-dependency": "^1.0.0",
    "fengwuxp-routing-antd": "^1.0.0",
    "fengwuxp-routing-core": "^1.0.0",
    "fengwuxp-routing-react": "^1.0.0",
    "fengwuxp-spring-react": "^1.0.0",
    "fengwuxp-styles-dependency": "^1.0.0",
    "fengwuxp-typescipt-dependency": "^1.0.0",
    "fengwuxp-typescript-feign": "^1.0.0",
    "fengwuxp-webpack-4.x-dependency": "^1.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "oak-common": "^1.0.0",
    "omit.js": "^1.0.2",
    "path-to-regexp": "2.4.0",
    "qs": "^6.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.4",
    "umi": "3.1.1",
    "umi-request": "^1.3.5"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^1.0.18",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^25.1.0",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.13",
    "@umijs/fabric": "^2.0.5",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/preset-ui": "^2.0.9",
    "carlo": "^0.9.46",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "husky": "^4.0.7",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.0.1",
    "pro-download": "1.0.1",
    "puppeteer-core": "^2.1.1",
    "stylelint": "^13.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "create-umi": {
    "ignoreScript": [
      "docker*",
      "functions*",
      "site",
      "generateMock"
    ],
    "ignoreDependencies": [
      "netlify*",
      "serverless"
    ],
    "ignore": [
      ".dockerignore",
      ".git",
      ".github",
      ".gitpod.yml",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "Dockerfile.*",
      "lambda",
      "LICENSE",
      "netlify.toml",
      "README.*.md",
      "azure-pipelines.yml",
      "docker",
      "CNAME",
      "create-umi"
    ]
  }
}

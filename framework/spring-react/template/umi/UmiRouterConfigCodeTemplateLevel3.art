import {IRoute} from "umi-types/config";
import {ViewShowMode} from "fengwuxp-routing-core";

const routes: IRoute[] =[
                {{each routes route index }}
                 {
                               name: '{{route.name}}管理',
                               {{if route.icon!=null}}
                                icon: {{route.icon}},
                                {{/if}}
                                path: '{{route.path}}',
                               routes: [
                                 {
                                 name: '{{route.name}}',
                                 {{if route.icon!=null}}
                                 icon: {{route.icon}},
                                 {{/if}}
                                  path: '{{route.path}}',
                                  routes: [
                                           {{each route.routes route index }}
                                            {
                                              name: '{{route.name}}',
                                              {{if route.icon!=null}}
                                                icon: {{route.icon}},
                                               {{/if}}
                                              path: '{{route.pathname}}',
                                              {{if route.condition}}
                                                  condition: {{route.condition}},
                                              {{/if}}
                                              {{if route.showMode}}
                                                showMode:{{route.showMode}},
                                              {{/if}}
                                              component: '{{route.component}}',
                                            },
                                    {{/each}}
                                   ]
                                 }
                              ]
                 },

               {{/each}}
    ]


export default routes
